{
  "name": "@monorepo/database",
  "version": "0.1.0",
  "private": true,
  "description": "Database logic and connection for @monorepo/server",
  "main": "src/index",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc",
    "lint": "prettier --write './src/**/*.ts'",
    "faker": "sequelize-cli db:seed:all --debug",
    "faker:clean": "sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "mysql2": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.3",
    "sequelize-typescript": "^2.0.0-beta.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^14.0.23",
    "@types/shortid": "^0.0.29",
    "@types/validator": "^13.1.0",
    "faker": "^4.1.0",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "typescript": "^3.9.7"
  }
}

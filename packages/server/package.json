    "express": "^4.17.1",
    "helmet": "^3.23.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/helmet": "^0.0.47",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}

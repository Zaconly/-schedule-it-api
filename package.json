    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.3.2",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.10",
    "nodemailer-express-handlebars": "^4.0.0",
